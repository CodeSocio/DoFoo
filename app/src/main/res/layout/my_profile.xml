<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MyProfile">

    <ImageView
        android:id="@+id/imageView75"
        android:layout_width="411dp"
        android:layout_height="260dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView64"
        android:layout_width="666dp"
        android:layout_height="821dp"
        android:layout_marginTop="180dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rectangle_479" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="439dp"
        android:layout_height="97dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/union_4"
        app:tint="#FCF4EFEF" />

    <ImageView
        android:id="@+id/imageView52"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="28dp"
        android:onClick="gotoMain"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/group_6815" />

    <ImageView
        android:id="@+id/imageView53"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView52"
        app:srcCompat="@drawable/group_6816" />

    <ImageView
        android:id="@+id/imageView54"
        android:layout_width="55dp"
        android:layout_height="50dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="22dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView55"
        app:srcCompat="@drawable/my_profile_button_selected" />

    <ImageView
        android:id="@+id/imageView55"
        android:layout_width="55dp"
        android:layout_height="45dp"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="27dp"
        android:onClick="gotoSettings"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/group_6818" />

    <ImageView
        android:id="@+id/imageView63"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginBottom="48dp"
        android:onClick="gotoMain"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView29"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView29"
        app:srcCompat="@drawable/home_btn" />

    <ImageView
        android:id="@+id/imageView68"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="155dp"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:onClick="gotoEditProfile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/group_8171" />

    <TextView
        android:id="@+id/charityName"
        android:layout_width="350dp"
        android:layout_height="30dp"
        android:layout_marginTop="70dp"
        android:fontFamily="casual"
        android:text=""
        android:textColor="#0B0B0B"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/imageView64"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView64"
        app:layout_constraintTop_toTopOf="@id/imageView64" />

    <TextView
        android:id="@+id/charityType"
        android:layout_width="350dp"
        android:layout_height="24dp"
        android:layout_marginTop="105dp"
        android:fontFamily="casual"
        android:text=""
        app:layout_constraintEnd_toStartOf="@+id/imageView64"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView64"
        app:layout_constraintTop_toTopOf="@id/imageView64" />

    <ImageView
        android:id="@+id/imageView71"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="250dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView64"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView64"
        app:layout_constraintTop_toTopOf="@id/imageView64"
        app:srcCompat="@drawable/line_35" />

    <!-- TODO change custom button here **UI**-->
    <ImageView
        android:id="@+id/imageView73"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="270dp"
        android:onClick="editUserListing"
        app:layout_constraintEnd_toStartOf="@+id/imageView64"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView64"
        app:layout_constraintTop_toTopOf="@id/imageView64"
        app:srcCompat="@drawable/view_my_listings_button" />

    <ImageView
        android:id="@+id/imageView74"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="340dp"
        android:onClick="gotoCreateReq"
        app:layout_constraintEnd_toStartOf="@+id/imageView64"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView64"
        app:layout_constraintTop_toTopOf="@id/imageView64"
        app:srcCompat="@drawable/sign_up_button_1" />

    <TextView
        android:id="@+id/phoneBanner"
        android:layout_width="58dp"
        android:layout_height="20dp"
        android:layout_marginTop="152dp"
        android:text="Phone:"
        android:textColor="#050505"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/charityType"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/charityType"
        app:layout_constraintTop_toTopOf="@+id/imageView64" />

    <TextView
        android:id="@+id/addressBanner"
        android:layout_width="58dp"
        android:layout_height="20dp"
        android:layout_marginTop="188dp"
        android:text="Address:"
        android:textColor="#050505"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/charityType"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/charityType"
        app:layout_constraintTop_toTopOf="@+id/imageView64" />

    <TextView
        android:id="@+id/phone"
        android:layout_width="271dp"
        android:layout_height="23dp"
        android:layout_marginTop="152dp"
        android:text=""
        android:textColor="#0B0B0B"
        app:layout_constraintEnd_toEndOf="@+id/charityType"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/charityType"
        app:layout_constraintTop_toTopOf="@+id/imageView64" />

    <TextView
        android:id="@+id/address"
        android:layout_width="271dp"
        android:layout_height="23dp"
        android:layout_marginTop="188dp"
        android:text=""
        android:textColor="#0B0B0B"
        app:layout_constraintEnd_toEndOf="@+id/charityType"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/charityType"
        app:layout_constraintTop_toTopOf="@+id/imageView64" />

    <ProgressBar
        android:id="@+id/profileProgressBar"
        style="?android:attr/progressBarStyle"
        android:layout_width="117dp"
        android:layout_height="94dp"
        android:layout_marginTop="316dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>